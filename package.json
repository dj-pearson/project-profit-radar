  "scripts": {
    "dev": "vite",
    "build": "npm run generate-sitemap && vite build && node scripts/copy-404.js && node scripts/update-sw-version.js",
    "build:dev": "npm run generate-sitemap && vite build --mode development && node scripts/copy-404.js",
    "lint": "eslint .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "preview": "vite preview",
    "build:prod": "npm run generate-sitemap && vite build --mode production && node scripts/copy-404.js",
    "build:cloudflare": "npm run generate-sitemap && vite build --mode production && node scripts/copy-404.js",
    "cloudflare:build": "node cloudflare-build.js",
    "cloudflare:install": "npm install --force && npm run build",
    "build:analyze": "npm run generate-sitemap && vite build --mode production && npx vite-bundle-analyzer dist/stats.html",
    "performance:audit": "node scripts/performance-audit.js",
    "performance:budget": "node scripts/check-performance-budget.js",
    "images:optimize": "node scripts/convert-images-to-webp.js",
    "performance:ci": "npm run build && npm run performance:budget && npm run performance:audit",
    "lighthouse": "lhci autorun",
    "lighthouse:collect": "lhci collect",
    "lighthouse:assert": "lhci assert",
    "lighthouse:full": "npm run build && lhci autorun",
    "generate-sitemap": "node scripts/generate-sitemap.js",
    "mcp:puppeteer": "npx @modelcontextprotocol/server-puppeteer",
    "mcp:context7": "npx @upstash/context7-mcp",
    "mcp:supabase": "npx -y @supabase/mcp-server-supabase@latest --project-id ilhzuvemiuyfuxfegtlv --access-token \"${SUPABASE_ACCESS_TOKEN}\"",
    "security-check": "powershell -ExecutionPolicy Bypass -File scripts/security-check.ps1",
    "analyze": "node scripts/analyze-bundle.js",
    "optimize:fonts": "powershell -ExecutionPolicy Bypass -File scripts/optimize-fonts.ps1",
    "optimize:fonts:unix": "chmod +x scripts/optimize-fonts.sh && ./scripts/optimize-fonts.sh",
    "mobile": "cd mobile-app && npm start",
    "mobile:install": "cd mobile-app && npm install",
    "mobile:start": "cd mobile-app && npm start",
    "mobile:android": "cd mobile-app && npm run android",
    "mobile:ios": "cd mobile-app && npm run ios",
    "mobile:prebuild": "cd mobile-app && npm run prebuild",
    "mobile:prebuild:clean": "cd mobile-app && npm run prebuild:clean",
    "mobile:build:dev:ios": "cd mobile-app && npm run build:dev:ios",
    "mobile:build:dev:android": "cd mobile-app && npm run build:dev:android",
    "mobile:build:preview:ios": "cd mobile-app && npm run build:preview:ios",
    "mobile:build:preview:android": "cd mobile-app && npm run build:preview:android",
    "mobile:build:prod:ios": "cd mobile-app && npm run build:prod:ios",
    "mobile:build:prod:android": "cd mobile-app && npm run build:prod:android",
    "mobile:submit:ios": "cd mobile-app && npm run submit:ios",
    "mobile:submit:android": "cd mobile-app && npm run submit:android"
